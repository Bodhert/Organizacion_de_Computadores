WORKDIR = `pwd`

CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = -Wall -fexceptions
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

INC_RELEASE = $(INC) -Iinclude
CFLAGS_RELEASE = $(CFLAGS) -O2
RESINC_RELEASE = $(RESINC)
RCFLAGS_RELEASE = $(RCFLAGS)
LIBDIR_RELEASE = $(LIBDIR)
LIB_RELEASE = $(LIB)
LDFLAGS_RELEASE = $(LDFLAGS) -s
OBJDIR_RELEASE = .objs
DEP_RELEASE = 
OUT_RELEASE = vmToasm

OBJ_RELEASE = $(OBJDIR_RELEASE)/CodeWriter.o $(OBJDIR_RELEASE)/Parser.o $(OBJDIR_RELEASE)/main.o

all: release

clean: clean_release

before_release: 
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) $(LIBDIR_RELEASE) -o $(OUT_RELEASE) $(OBJ_RELEASE)  $(LDFLAGS_RELEASE) $(LIB_RELEASE)

$(OBJDIR_RELEASE)/CodeWriter.o: CodeWriter.cpp
	$(CXX) $(CFLAGS_RELEASE) -std=c++11 $(INC_RELEASE)  -c CodeWriter.cpp -o .objs/CodeWriter.o

$(OBJDIR_RELEASE)/Parser.o: Parser.cpp
	$(CXX) $(CFLAGS_RELEASE) -std=c++11 $(INC_RELEASE) -c Parser.cpp -o .objs/Parser.o

$(OBJDIR_RELEASE)/main.o: main.cpp
	$(CXX) $(CFLAGS_RELEASE) -std=c++11 $(INC_RELEASE) -c main.cpp -o .objs/main.o

clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf $(OBJDIR_RELEASE)

.PHONY:  before_release after_release clean_release

